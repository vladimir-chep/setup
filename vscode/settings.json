{

    "workbench.colorTheme": "Shades of Purple",
    "workbench.activityBar.visible": true,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.fontAliasing": "auto",
    "workbench.startupEditor": "newUntitledFile",
    "workbench.colorCustomizations": {
        // "terminal.foreground" : "#ffffff",
        // "terminal.background" : "#151515",
    },

    "editor.largeFileOptimizations": false,
    "editor.minimap.showSlider": "always",
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.wordWrap": "on",
    "editor.parameterHints.cycle": true,
    "editor.smoothScrolling": true,
    "editor.renderControlCharacters": true,
    "editor.renderLineHighlight": "all",
    "editor.renderWhitespace": "all",
    // "editor.fontFamily": "Operator Mono, Menlo, Monaco, 'Courier New', monospace",
    // "editor.fontFamily": "'Cascadia Code', 'Fira Code', 'Anonymous Pro', Menlo, Monaco, 'Courier New', monospace",
    "editor.fontFamily": "'Fira Code', 'Anonymous Pro', Menlo, Monaco, 'Courier New', monospace",
    "editor.fontSize": 17,
    "editor.fontLigatures": true,
    "editor.lineHeight": 24,
    "editor.fontWeight": "400",
    "editor.letterSpacing": 0.5,
    "editor.cursorStyle": "line",
    "editor.cursorWidth": 5,
    "editor.cursorBlinking": "solid",
    "editor.snippetSuggestions": "top",
    "editor.glyphMargin": true,
    "editor.formatOnPaste": false,
    "editor.formatOnSave": false,
    "editor.formatOnType": false,
    "editor.tokenColorCustomizations": {
        "textMateRules": [{
                "scope": "keyword",
                "settings": {
                    "fontStyle": "bold"
                }
            },
            {
                "scope": "storage",
                "settings": {
                    "fontStyle": "bold"
                }
            },
            {
                "scope": "constant.language",
                "settings": {
                    "fontStyle": "bold"
                }
            },
            {
                "scope": "support.class.builtin",
                "settings": {
                    "fontStyle": "bold"
                }
            }
        ]
    },

    "window.zoomLevel": 0,

    "zenMode.hideTabs": false,

    "breadcrumbs.enabled": true,

    "terminal.integrated.fontFamily": "Cascadia Code, DejaVuSansMono Nerd Font Mono, Consolas, 'Courier New', monospace",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.macOptionIsMeta": true,

    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.decorations.badges": false,

    "auto-close-tag.fullMode": true,
    "auto-close-tag.SublimeText3Mode": true,

    "emmet.showSuggestionsAsSnippets": true,
    "emmet.triggerExpansionOnTab": true,

    "vetur.format.defaultFormatter.js": "prettier-eslint",
    "vetur.format.options.tabSize": 4,
    "vetur.format.defaultFormatter.html": "js-beautify-html",
    "vetur.format.defaultFormatterOptions": {
        "js-beautify-html": {
            "wrap_attributes": "force-aligned"
        }
    },


    "files.trimTrailingWhitespace": true,
    "files.trimFinalNewlines": true,

    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,

    "highlight-matching-tag.styles": {
        "opening": {
            "full": {
                "custom": {
                    "backgroundColor": "rgba(165, 153, 2, 0.20)"
                }
            },
            "left": {
                "custom": {
                    "borderWidth": "0"
                }
            },
            "right": {
                "custom": {
                    "borderWidth": "0"
                }
            }
        }
    },

    "importCost.largePackageColor": "#EC3A37F5",
    "importCost.mediumPackageColor": "#B362FF",
    "importCost.smallPackageColor": "#B362FF",

    "quokka.colors": {
        // "covered": "#62b455",
        // "errorPath": "#ffa0a0",
        // "errorSource": "#fe536a",
        // "notCovered": "#cccccc",
        // "partiallyCovered": "#d2a032"
    },

    "eslint.enable": true,
    "eslint.options": {
        "configFile": "/usr/local/lib/node_modules/eslint/lib/.eslintrc.js"
        // "baseConfig": "/usr/local/lib/node_modules/eslint/lib/.eslintrc.js",
    },
    "eslint.validate": [ "vue", "javascript", "javascriptreact", "html" ],

    "beautify.config": {
        "allowed_file_extensions": ["js", "json", "jshintrc", "jsbeautifyrc"],
        "brace_style": "collapse",
        "e4x": true,
        "indent_size": 4,
        "indent_char": " ",
        "indent_with_tabs": false,
        "indent_level": 0,
        "editorconfig": false,
        "eol": "\n",
        "end_with_newline": false,
        "preserve_newlines": true,
        "max_preserve_newlines": 3,
        "wrap_line_length": 120,
        "break_chained_methods": false,
        "comma_first": false,
        "jslint_happy": false,
        "keep_array_indentation": false,
        "operator_position": "before-newline",
        "space_after_anon_function": true,
        "space_after_named_function": true,
        "space_in_empty_paren": false,
        "space_in_paren": false,
        "unindent_chained_methods": false,
        "unescape_strings": false
    },
    "csscomb.preset": {
        "remove-empty-rulesets": true,
        "always-semicolon": true,
        "color-case": "lower",
        "block-indent": "    ",
        "color-shorthand": true,
        "element-case": "camel",
        "eof-newline": true,
        "leading-zero": false,
        "quotes": "single",
        "space-before-colon": "",
        "space-after-colon": " ",
        "space-before-combinator": " ",
        "space-after-combinator": " ",
        "space-between-declarations": "\n",
        "space-before-opening-brace": " ",
        "space-after-opening-brace": "\n",
        "space-before-selector-delimiter": "",
        "space-after-selector-delimiter": "\n",
        "space-before-closing-brace": "\n",
        "strip-spaces": true,
        "tab-size": true,
        "unitless-zero": true,
        "vendor-prefix-align": true,
        "lines-between-rulesets": 1,
        "sort-order": [
            [
                "$import",
                "$variable",
                "$extend",
